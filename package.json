{
  "name": "fastify-remix-demo",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build && tsc --project ./tsconfig.server.json",
    "dev": "cross-env ENV=local tsx --watch-path ./server/index.ts ./server/index.ts",
    "format": "prettier --ignore-path .gitignore --ignore-unknown --cache --cache-location node_modules/.cache/prettiercache --write .",
    "lint": "eslint --ignore-path .gitignore --no-error-on-unmatched-pattern --cache --cache-location node_modules/.cache/eslint --fix .",
    "start": "cross-env ENV=prod node ./server/index.js",
    "typecheck": "tsc",
    "cdk:deploy:infra:it": "cdk deploy InfraStack-it -c env=it",
    "cdk:synth:infra:it": "cdk synth InfraStack-it -c env=it"
  },
  "dependencies": {
    "@fastify/autoload": "^6.0.1",
    "@fastify/aws-lambda": "^5.0.0",
    "@fastify/middie": "^8.3.1",
    "@fastify/static": "^7.0.4",
    "@mcansh/remix-fastify": "3.4.0",
    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "@remix-run/server-runtime": "^2.12.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "fastify-plugin": "^5.0.1",
    "isbot": "^5.1.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "source-map-support": "^0.5.21",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.12.1",
    "@remix-run/eslint-config": "^2.12.1",
    "@types/jest": "^29.5.12",
    "@types/node": "22.5.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/source-map-support": "^0.5.10",
    "aws-cdk": "2.160.0",
    "aws-cdk-lib": "2.160.0",
    "constructs": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "^5.6.2",
    "vite": "^5.3.2",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
